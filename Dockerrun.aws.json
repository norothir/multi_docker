{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        { "name" : "client",
            "image":"norothir/multi-client",
            "hostname":"client",
            "essential":false
        },
        { "name" : "server",
            "image":"norothir/multi-server",
            "hostname":"api",
            "essential":false
        },
        { "name" : "worker",
            "image":"norothir/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"norothir/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }],

                "links":["client","server"]
                
            
        }
    ]

}